{
  "name": "css-animation-sync",
  "version": "0.0.7",
  "description": "Synchronise and control CSS Animations",
  "main": "dist/sync.js",
  "peerDependencies": {},
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-add-module-exports": "^1.0.2",
    "bootstrap-css-only": "^3.3.7",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jsdom": "^15.2.0",
    "mocha": "^3.5.3",
    "parcel-bundler": "^1.12.4",
    "sinon": "^7.5.0"
  },
  "scripts": {
    "build": "parcel build ./src/sync.js --out-dir ./dist --global sync",
